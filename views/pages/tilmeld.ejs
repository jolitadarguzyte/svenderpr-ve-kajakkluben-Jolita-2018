<!DOCTYPE html>
<html>

<head>
  <title>
    <%= title %>
  </title>
</head>

<body>
  <header>
    <% include ../partials/css %>
      <% include ../partials/header %>
  </header>
  <div id="kontaktBody">
    <div id="indhold6">
      <h3 id="titleH3">
        <%= title %>
      </h3>

      <div id="indhold5">
        <div id="kontaktForm">
          <form id="contact" action="" method="post">
            <input class="kontaktInput" placeholder="* Navn" type="text" tabindex="1" required autofocus>
            <input class="kontaktInput" placeholder="* Email" type="email" tabindex="2" required>
            <input class="kontaktInput" placeholder="* Telefonnummer" type="tel" tabindex="3" required>
            <textarea placeholder="* Skriv vendligst antal peroner" tabindex="5" required></textarea>
            <button name="submit" type="submit" id="contact-submit" data-submit="...Sending">Send tilmelding</button>
          </form>
        </div>

      </div>
    </div>
  </div>
  <footer>
    <% include ../partials/footer %>
  </footer>
</body>

</html>