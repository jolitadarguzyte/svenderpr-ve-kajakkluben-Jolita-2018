<!DOCTYPE html>

<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-T8Gy5hrqNKT+hzMclPo118YTQO6cYprQmhrYwIiQ/3axmI1hQomh7Ud2hPOy8SP1"
    crossorigin="anonymous">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  <link rel='stylesheet' href='/stylesheet/admin.css' />
  <title>Administration</title>
  <% include ../partials/css %>
</head>

<body class="home">
  <div class="container top-container">
    <div class="navbar navbar-inverse">
      <div class="container-fluid">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#nav-bar" aria-expanded="false">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/admin">Arrangementer</a>
          <a class="navbar-brand" href="/adminGalleri"> Galleri</a>
          <a class="navbar-brand" href="/adminNyheder">Nyheder</a>
          <a class="navbar-brand" href="/adminBader">Både</a>
          <a class="navbar-brand" href="/adminMedlem"> Medlemmer</a>
          <a class="navbar-brand" href="/kontakt">Kontakt</a>
            <span class="glyphicon glyphicon-envelope"></span> &nbsp; Beskeder</a>

        </div>
        <ul class="nav navbar-nav navbar-right">
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Logud
              <span class="caret"></span>
            </a>
            <ul id="nav_login" class="dropdown-menu login-nav-dropdown">

              <div class="container-fluid dropdown-header">
                <div class="row">
                  <div class="col-xs-12 col-md-8">
                    <strong>Logoud</strong>
                  </div>
                  <div class="col-xs-12 col-md-8">&nbsp;</div>
                  <div class="col-xs-12">
                    <a href="logud" name="" class="btn">
                      <a href="/logout">Log Ud</a>
                    </a>
                  </div>
                  <div class="col-xs-12">
                    <form class="form-horizontal loginForm">
                      <fieldset>
                        &nbsp;
                      </fieldset>
                    </form>

                  </div>
                </div>
              </div>
      </div>
    </div>
  </div>
  <h1 style="color:rgb(62, 60, 60);  text-align: center;">Welkomen til Medlemside </h1>
  <h4 style="color:rgb(62, 60, 60);  text-align: center;">Her du kan slette,redigere eller oprete Medlemmer</h4>
  <h4 style="color:green; text-align: center;">Administrators rettiheder svar til tal: 1</h4>
  <h4 style="color:green; text-align: center;">Instruktørs rettiheder svar til tal: 2</h4>
  <h4 style="color:green; text-align: center;">Medlems rettiheder svar til tal: 3</h4>


  <div class="container">

    <div id="alert"></div>
    <div class="panel-body col-md-6">
      <ul class="list-group" id="products">
        <% users.forEach(function(users) { %>
          <br>

          <li class="list-group-item product">
            <div class="row">
              <div class="col-sm-12 product_img img-admin-product">

                <img class="img6" src="/images/<%= users.foto %>" alt="">
                <h3>
                  <%= users.navn %>
                </h3>
                <p>
                  <%= users.email %>
                </p>
                <p>
                  <%= users.password %>
                </p>
                <p>
                  <%= users.rettiheder %>
                </p>

              </div>
              <div class="col-xs-10 col-md-11">
                <div>
                  <p class="producttitle">
                    <p class="cykelp"></p>

                  </p>
                </div>
                <div class="product-description">
                  <p class="productdescription">

                  </p>
                </div>
                <div class="product-price">
                  <p class="pricetext">
                  </p>
                </div>
                <div class="action">
                  <a href="#" type="button" class="btn btn-primary btn-xs edit" data-id="" title="Rediger">
                    <span class="glyphicon glyphicon-pencil"></span>
                  </a>
                  <a href="#" type="button" class="btn btn-danger btn-xs delete" data-id="" title="Slet">
                    <span class="glyphicon glyphicon-trash"></span>
                  </a>
                </div>
              </div>
            </div>
          </li>
          <% }) %>
      </ul>
    </div>

    <div class="panel-body col-md-6">
      <form class="form-horizontal">
        <fieldset>

          <!-- Form Name -->
          <legend style="color:red;">Rediger Medlem</legend>


          <!-- FOTO input-->
          <div class="form-group">
            <label class="col-md-6 control-label" for="url">URL</label>
            <div class="col-md-6">
              <input id="url" name="url" placeholder="Billede" class="input-file" required="" type="file" value="billede.jpg">

            </div>
          </div>


          <!-- Text input-->
          <div class="form-group">
            <label class="col-md-6 control-label" for="farve">Navn</label>
            <div class="col-md-6">
              <input id="farve" name="farve" placeholder="Navn:" class="form-control input-md" required="" type="text">
            </div>
          </div>

          <!-- Text input-->
          <div class="form-group">
            <label class="col-md-6 control-label" for="pris">Email</label>
            <div class="col-md-6">
              <input id="pris" name="pris" placeholder="Email:" class="form-control input-md" required="" type="text">

            </div>
          </div>

          <!-- Text input-->
          <div class="form-group">
            <label class="col-md-6 control-label" for="pris">Password</label>
            <div class="col-md-6">
              <input id="pris" name="pris" placeholder="Password:" class="form-control input-md" required="" type="text">

            </div>
          </div>

          <!-- Text input-->
          <div class="form-group">
            <label class="col-md-6 control-label" for="farve">Rettiheder</label>
            <div class="col-md-6">
              <input id="farve" name="farve" placeholder="" class="form-control input-md" required="" type="number">
            </div>
          </div>

          <!-- Button -->
          <div class="form-group">
            <label class="col-md-6 control-label" for="singlebutton">Gem ændringer</label>
            <div class="col-md-6">
              <button id="singlebutton" name="singlebutton" class="btn btn-primary">Gem</button>


            </div>
          </div>

        </fieldset>
      </form>

    </div>

    <div class="panel-body col-md-6">
      <form class="form-horizontal">
        <fieldset>

          <!-- Form Name -->
          <legend style="color:red;">Oprete Medlem</legend>


         <!-- FOTO input-->
         <div class="form-group">
            <label class="col-md-6 control-label" for="url">URL</label>
            <div class="col-md-6">
              <input id="url" name="url" placeholder="Billede" class="input-file" required="" type="file" value="billede.jpg">

            </div>
          </div>


          <!-- Text input-->
          <div class="form-group">
            <label class="col-md-6 control-label" for="farve">Navn</label>
            <div class="col-md-6">
              <input id="farve" name="farve" placeholder="Navn:" class="form-control input-md" required="" type="text">
            </div>
          </div>

          <!-- Text input-->
          <div class="form-group">
            <label class="col-md-6 control-label" for="pris">Email</label>
            <div class="col-md-6">
              <input id="pris" name="pris" placeholder="Email:" class="form-control input-md" required="" type="text">

            </div>
          </div>

          <!-- Text input-->
          <div class="form-group">
            <label class="col-md-6 control-label" for="pris">Password</label>
            <div class="col-md-6">
              <input id="pris" name="pris" placeholder="Password:" class="form-control input-md" required="" type="text">

            </div>
          </div>

          <!-- Text input-->
          <div class="form-group">
            <label class="col-md-6 control-label" for="farve">Rettiheder</label>
            <div class="col-md-6">
              <input id="farve" name="farve" placeholder="" class="form-control input-md" required="" type="number">
            </div>
          </div>

          <!-- Button -->
          <div class="form-group">
            <label class="col-md-6 control-label" for="singlebutton">Gem ændringer</label>
            <div class="col-md-6">
              <button id="singlebutton" name="singlebutton" class="btn btn-primary">Gem</button>


            </div>
          </div>

        </fieldset>
      </form>

    </div>


  </div>
  <script src="/javascripts/delete.js"></script>
  <script src="/javascripts/redigereProdukter.js"></script>
</body>

</html>